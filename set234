SET 2
	1.
	sudo groupadd mygroup
	sudo useradd -m -g mygroup owner
	hostname -I
	
	2.
	touch fileA
	ln fileA fileA_hard
	ln -s fileA fileA_soft
	ls -l




	3.
	nano lcount.awk
	
	echo "Enter a file name"
	read fn
	awk '
	$0 !~ /[aeiouAEIOU]/ { c=c+1 }
	END { print "The number of lines that do not contain vowels:", c }
	' "$fn"
	
	chmod +x lcount.awk
	
	cat > fname
	linux
	xyz
	abc
	rhythm
	^D
	
	sh lcount.awk





	4.
	nano cwlcount.awk
	
	echo "Enter a file name"
	read fn

	awk '
	{
   	 w = w + NF            # NF = number of fields = number of words in line
   	 c = c + length($0)    # number of characters in the line
	}
	END {
    	print "No.of lines:", NR
    	print "No.of words:", w
    	print "No.of characters:", c
	}
	' "$fn"

	chmod +x cwlcount.awk

	cat > sample.sh
	This is a test file
	It has five lines
	and some words
	to count using
	awk program
	^D
	
	sh cwlcount.awk

	5.
	sed 's/linux/unix/g' os.txt
 

SET 3 
	1.
	ps -ef

	ps -ef|grep sleep

	kill-9 pid

	sleep 200&

	2.
	ping -c 4 ip

	ssh user@ip

	ifconfig
	hostname
	who

	3.
	nano range.sh

	echo "Enter file name"
	read f
	echo "Enter starting line number"
	read st
	echo "Enter ending line number"
	read en
	echo "The lines between $st and $en from $f are:"
	if [ $st -lt $en ]
	then
    	n1=`expr $st + 1`
    	n2=`expr $en - 1`
    	sed -n "$n1,$n2 p" $f
	elif [ $st -gt $en ]
	then
    	n3=`expr $en + 1`
    	n4=`expr $st - 1`
    	sed -n "$n3,$n4 p" $f
	fi

	chmod +x range.sh

	./range.sh


SET 4

	1.
	cat > sample.txt

	sed -n '5,12p' sample.txt

	2.
	cat > original.txt

	cp original.txt backup.txt

	ls-l

	3.
	grep -v "linux" sample.txt

	4.
	nano deleteword.sh

	echo "Enter the word to delete:"
	read word
	for file in "$@"
	do
    	sed -i "/$word/d" "$file"
	done
	echo "Lines containing '$word' have been deleted."

	chmod +x deleteword.sh

	./deleteword.sh f1.txt f2.txt